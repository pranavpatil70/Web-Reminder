{
  "manifest_version": 3,
  "name": "Web Reminder Extension",
  "version": "1.0",
  "description": "A web extension that lets you save notes, URLs, and set reminders.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "./index.html",
    "default_icon": {
  
    },
    "default_popup_width": 400,   
    "default_popup_height": 600   
  },
  "icons": {

  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self'"
  }
}
